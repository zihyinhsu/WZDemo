@model IEnumerable<WZDemo.UI.Models.DTO.RegionDto>

@{
}

<h1 class="mt-3">Regions</h1>
<table class="table talbe-bordered">
	<thead>
		<tr>
			<th>Id</th>
			<th>Code</th>
			<th>Name</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Code</td>
				<td>@item.Name</td>
			</tr>
		}
	</tbody>
</table>